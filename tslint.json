{
  "defaultSeverity": "error",
  "extends": "tslint-eslint-rules",
  "jsRules": {
      "quotemark": false
  },
  "rules": {
      "max-line-length": [
          true,
          {"limit": 80, "ignore-pattern": "^import |^export {(.*?)}"}
      ],
      "interface-name": false,
      "one-variable-per-declaration": false,
      "prefer-for-of": false
  },
  "rulesDirectory": []
}
